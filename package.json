{
    "name": "klarna-checkout-sdk",
    "version": "1.2.4",
    "main": "commonjs/index.js",
    "types": "esm/@types/types.d.ts",
    "module": "esm/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Romkin7/klarna-checkout-sdk"
    },
    "scripts": {
        "start": "tsc --watch",
        "build": "rimraf ./dist && tsc -p ./tsconfig.json && tsc -p ./tsconfig.commonjs.json && yarn copyPackageJson",
        "copyPackageJson": "node scripts/copyPackageJson.js",
        "test:coverage": "jest --coverage --colors --detectOpenHandles --forceExit",
        "test": "jest --colors --detectOpenHandles --runInBand --forceExit",
        "test:watch": "jest --coverage --colors --watchAll",
        "lint": "yarn run prettier:test && eslint src/**/*.ts --ext .ts{x}",
        "prettier:test": "prettier --check \"src/**/*.ts*\"",
        "prettier:fix": "prettier --write \"src/**/*.ts*\"",
        "publish": "npm publish --access public"
    },
    "author": {
        "email": "devdesignrt@devdesign.fi",
        "name": "DevDesing",
        "url": "https://www.devdesign.fi"
    },
    "engines": {
        "node": ">=16.*",
        "yarn": ">=1.*"
    },
    "license": "MIT",
    "dependencies": {
        "axios": "0.27.2"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.21.0",
        "@typescript-eslint/parser": "5.21.0",
        "@types/jest": "27.4.1",
        "eslint": "8.14.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "4.0.0",
        "jest": "27.5.1",
        "prettier": "2.6.2",
        "rimraf": "3.0.2",
        "ts-jest": "27.1.4",
        "ts-node": "10.7.0",
        "typescript": "4.7.4"
    },
    "lint-staged": {
        "*.{tsx,ts,js,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "proseWrap": "always"
    }
}
